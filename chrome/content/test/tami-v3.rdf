<?xml version="1.0"?> 

<!DOCTYPE rdf [
  <!ENTITY	rdf	"http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
  <!ENTITY	rdfs	"http://www.w3.org/2000/01/rdf-schema#" >
  <!ENTITY	xsd	"http://www.w3.org/2001/XMLSchema" >
  <!ENTITY	u	"http://niem.gov/niem/universal/0.1" >
  <!ENTITY	s	"http://niem.gov/niem/structures/0.1" >
  <!ENTITY	c	"http://niem.gov/niem/common/0.1" >
  <!ENTITY	j	"http://www.niem.gov/niem/domains/justice/0.1" >
  <!ENTITY	emer	"http://www.niem.gov/niem/domains/emergency-management/0.1" >
  <!ENTITY	int	"http://www.niem.gov/niem/domains/intelligence/0.1" >
  <!ENTITY	air	"http://www.megginson.com/exp/ns/airports#" >
]>

<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:u="http://niem.gov/niem/universal/0.1"
  xmlns:s="http://niem.gov/niem/structures/0.1"
  xmlns:c="http://niem.gov/niem/common/0.1"
  xmlns:j="http://www.niem.gov/niem/domains/justice/0.1"
  xmlns:emer="http://www.niem.gov/niem/domains/emergency-management/0.1"
  xmlns:int="http://www.niem.gov/niem/domains/intelligence/0.1"
  xmlns:air="http://www.megginson.com/exp/ns/airports#">

<rdfs:Class rdf:ID="scenario"/>

<rdfs:comment> Agents and Suspects are Actors ................. </rdfs:comment>

<rdfs:Class rdf:ID="actor"/>

<rdfs:Class rdf:ID="agent">
 <rdfs:subClassOf rdf:resource="#actor"/>
</rdfs:Class>

<rdfs:Class rdf:ID="suspect">
 <rdfs:subClassOf rdf:resource="#actor"/>
</rdfs:Class>

<rdfs:comment> Actors have Names ........................--</rdfs:comment>

<rdf:Property rdf:ID="&u;PersonGivenName">
  <rdfs:domain rdf:resource="#actor"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonMiddleName">
  <rdfs:domain rdf:resource="#actor"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonSurName">
  <rdfs:domain rdf:resource="#actor"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Suspects have Addresses ........................ </rdfs:comment>

<rdf:Property rdf:ID="&u;LocationAddress">
  <rdfs:domain rdf:resource="#suspect"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationCityName">
  <rdfs:domain rdf:resource="#suspect"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationStateName">
  <rdfs:domain rdf:resource="#suspect"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationPostalCodeID">
  <rdfs:domain rdf:resource="#suspect"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Agents work for Organizations ........................ </rdfs:comment>

<rdf:Property rdf:ID="organization">
  <rdfs:domain rdf:resource="#agent"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;EmploymentPositionName">
  <rdfs:domain rdf:resource="#agent"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Organizations ........................ </rdfs:comment>

<rdfs:Class rdf:ID="organization"/>

<rdf:Property rdf:ID="&u;EmploymentLocation">
  <rdfs:domain rdf:resource="#organization"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;OrganizationName">
  <rdfs:domain rdf:resource="#organization"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Databases ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="database"/>

<rdf:Property rdf:ID="&u;OrganizationName">
  <rdfs:domain rdf:resource="#database"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Events ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="event"/>

	<rdfs:comment> search events relate the search query with the search results and are also time-stamped </rdfs:comment>
	<rdfs:comment> search results are not time-stamped </rdfs:comment>
	<rdfs:comment> this is ok, if searches can be considered instantaneous </rdfs:comment>
	<rdfs:comment> if there can pass some time from the identification of the search-query to the actual execution
		it might be safer to time-stamp the search results </rdfs:comment>
	<rdfs:comment> this is especially true, if there are several databases to query from different institutions </rdfs:comment>


	<rdfs:comment> to be completed </rdfs:comment>

<rdfs:comment> Data-records ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="data-record"/>

<rdf:Property rdf:ID="type">
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>	<rdfs:comment> or restrict from a given set of possibilities </rdfs:comment>
</rdf:Property>

<rdf:Property rdf:ID="source">
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonGivenName">
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonMiddleName">
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonSurName">
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="dob">                <rdfs:comment> use better something else (eg. from vcard...)</rdfs:comment>
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="flight">		
  <rdfs:domain rdf:resource="#data-record"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>	<rdfs:comment> or from company codes </rdfs:comment>
</rdf:Property>

<rdf:Property rdf:ID="number">
  <rdfs:domain rdf:resource="#flight"/>
  <rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>

<rdf:Property rdf:ID="origination">
  <rdfs:domain rdf:resource="#flight"/>
  <rdfs:range rdf:resource="&air;iata"/>
</rdf:Property>

<rdf:Property rdf:ID="destination">		
  <rdfs:domain rdf:resource="#flight"/>
  <rdfs:range rdf:resource="&air;iata"/>
</rdf:Property>

<rdf:Property rdf:ID="date">			 <rdfs:comment> use better xsd:date </rdfs:comment>
  <rdfs:domain rdf:resource="#flight"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Searches ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="search-query"/>

<rdf:Property rdf:ID="search-term">
  <rdfs:domain rdf:resource="#search-query"/>
  <rdfs:range rdf:resource="#term"/>
</rdf:Property>

<rdf:Class rdf:ID="term"/>

<rdf:Property rdf:ID="&u;PersonGivenName">
  <rdfs:domain rdf:resource="#term"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonMiddleName">
  <rdfs:domain rdf:resource="#term"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonSurName">
  <rdfs:domain rdf:resource="#term"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="dob">                <rdfs:comment> use better something else (from vcard...)</rdfs:comment>
  <rdfs:domain rdf:resource="#term"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:Class rdf:ID="search-result"/>

<rdf:Class rdf:ID="item"/>

<rdf:Property rdf:ID="&u;PersonGivenName">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonMiddleName">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;PersonSurName">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="dob">                <rdfs:comment> use better something else (from vcard...)</rdfs:comment>
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationAddress">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationCityName">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationStateName">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="&u;LocationPostalCodeID">
  <rdfs:domain rdf:resource="#item"/>
  <rdfs:range rdf:resource="&rdf;Literal"/>
</rdf:Property>

<rdfs:comment> Transfer of content ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="transfer-content"/>

	<rdfs:comment> to be completed </rdfs:comment>

<rdfs:comment> Cases ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="case"/>

	<rdfs:comment> to be completed </rdfs:comment>

<rdfs:comment> Arrests ......................................... </rdfs:comment>

<rdfs:Class rdf:ID="j:Arrest"/>

	<rdfs:comment> to be completed </rdfs:comment>

</rdf:RDF>